{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "storage-test",
      "type": "dart",
      "request": "launch",
      "toolArgs": [
        "--define=DEBUG=true"
      ],
      "cwd": "${workspaceFolder}/storage/dart",
      "program": "${workspaceFolder}/storage/dart/test/test.dart",
      "preLaunchTask": "storage-native-build"
    },
    {
      "name": "storage-test-gdb",
      "type": "cppdbg",
      "request": "launch",
      "program": "/usr/bin/dart",
      "args": [
        "-DDEBUG=true",
        "${workspaceFolder}/storage/dart/test/test.dart"
      ],
      "stopAtEntry": false,
      "cwd": "${workspaceFolder}/storage/dart",
      "MIMode": "gdb",
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        }
      ],
      "preLaunchTask": "storage-native-build"
    },
