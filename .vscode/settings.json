{
  "cmake.buildDirectory": "${workspaceFolder}",
  "dart.sdkPath": "~/development/dart-sources/sdk/out/ReleaseX64/dart-sdk",
  "dotnet.defaultSolution": "disable",
  "git.autoRepositoryDetection": false,
  "mesonbuild.configureOnOpen": false,
  "C_Cpp.dimInactiveRegions": false,
  "clangd.checkUpdates": true,
  "clangd.onConfigChanged": "restart",
  "clangd.arguments": [
    "-background-index",
    "-header-insertion=iwyu",
    "-header-insertion-decorators",
    "--inlay-hints",
    "--function-arg-placeholders",
  ],
  "grammarly.selectors": [
    {
      "language": "mdx",
      "scheme": "file"
    }
  ],
  "C_Cpp.errorSquiggles": "disabled",
  "files.associations": {
    "*.tcc": "c",
    "bitset": "c",
    "chrono": "c",
    "algorithm": "c",
    "random": "c",
    "fstream": "c",
    "streambuf": "c",
    "*.inc": "c",
    "test.h": "c",
    "mediator_configuration.h": "c",
    "mediator_dart.h": "c",
    "assert.h": "c",
    "mhash.h": "c",
    "dart_native_api.h": "c",
    "cstdint": "c"
  },
}